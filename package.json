{
  "name": "websocket-benchmark",
  "version": "0.0.1",
  "description": "compare WebSocket Frameworks for different languages",
  "main": "index.js",
  "scripts": {
    "benchmark:scenarios": "k6 run ./benchmarks/scenarios.js",
    "benchmark:connection": "k6 run --vus 128 --duration 5s ./benchmarks/connection.js > ./results/connection.txt",
    "benchmark:plain-async": "k6 run --vus 128 --duration 5s ./benchmarks/plain-async.js > ./results/plain-async.txt",
    "benchmark:plain-sync": "k6 run --vus 128 --duration 5s ./benchmarks/plain-sync.js > ./results/plain-sync.txt",
    "benchmark:json-async": "k6 run --vus 128 --duration 5s ./benchmarks/json-async.js > ./results/json-async.txt",
    "benchmark:json-sync": "k6 run --vus 128 --duration 5s ./benchmarks/json-sync.js > ./results/json-sync.txt",
    "benchmark:binary-async": "k6 run --vus 128 --duration 5s ./benchmarks/binary-async.js > ./results/binary-async.txt",
    "benchmark:binary-sync": "k6 run --vus 128 --duration 5s ./benchmarks/binary-sync.js > ./results/binary-sync.txt",
    "benchmark:all": "npm run benchmark:connection && npm run benchmark:plain-async && npm run benchmark:plain-sync && npm run benchmark:json-async && npm run benchmark:json-sync && npm run benchmark:binary-async && npm run benchmark:binary-sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntsd/websocket-benchmark.git"
  },
  "author": "Jirawat Boonkumnerd <contact@ntsd.dev> (https://ntsd.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntsd/websocket-benchmark/issues"
  },
  "homepage": "https://github.com/ntsd/websocket-benchmark#readme",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@types/k6": "^0.47.1",
    "babel-loader": "^9.1.3",
    "core-js": "^3.33.2",
    "protobufjs": "^7.2.5",
    "ts-proto": "^1.163.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}